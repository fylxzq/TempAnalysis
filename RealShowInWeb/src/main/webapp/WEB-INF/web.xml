<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>tempWeb</display-name>
  <servlet>
    <servlet-name>TempServlet</servlet-name>
    <servlet-class>com.fy.servlet.TempServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TempServlet</servlet-name>
    <url-pattern>/TempServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>jsp/temp.jsp</welcome-file>
  </welcome-file-list>
</web-app>
